        <my:String x:Key="U.KeToan.ButToan.ucButToanDS.MaPhanHe">Mã phân hệ</my:String>
        <my:String x:Key="U.KeToan.ButToan.ucButToanDS.MaNghiepVu">Mã nghiệp vụ</my:String>
