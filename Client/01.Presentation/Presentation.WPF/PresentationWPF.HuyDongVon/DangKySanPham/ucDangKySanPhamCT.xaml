 Height="{Binding ElementName=grMain, Path=ActualWidth,
                                    Converter={uc:MathConverter}, ConverterParameter=x/5}" VerticalContentAlignment="Center" HorizontalContentAlignment="Left" IsSelected="True">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="100*"/>
                                        <!--<ColumnDefinition Width="5*"/>-->
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0">
                                        <DockPanel>
                                            <Border DockPanel.Dock="Top" BorderThickness="1" CornerRadius="4" Margin="3" Padding="13,5,10,5" BorderBrush="LightGray">
                                                <Grid VerticalAlignment="Center" HorizontalAlignment="Left">
                                                    <StackPanel Orientation="Horizontal">
                                                        <Label HorizontalAlignment="Left" Content="{DynamicResource U.DungChung.TrangThaiNghiepVu}"></Label>
                                                        <Label Name="lblTrangThai"></Label>
                                                    </StackPanel>
                                                </Grid>
                                            </Border>
                                            <GroupBox Header="{StaticResource U.HuyDongVon.ucDangKySanPhamCT.ThongTinSanPham}" DockPanel.Dock="Top" Margin="3" Padding="10,10,0,0">
                                                <Grid Name="gridThongTinSP">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="150"/>
                                                        <ColumnDefinition Width="2.3*"/>
                                                        <ColumnDefinition Width="20" />
                                                        <ColumnDefinition Width="150" />
                                                        <ColumnDefinition Width="2*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="6" />
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="6" />
                                                        <RowDefinition Height="*" />
                                                        <RowDefinition Height="6" />
                                                        <RowDefinition Height="*" />
                                                    </Grid.RowDefinitions>
                                                    <StackPanel Orientation="Horizontal">
                                                        <Label Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.NhomSP}" Name="lblNhomSP" />
                                                        <Label Content="(*)" Foreground="Red" Margin="-5,0,0,0" />
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal" Grid.Row="0" Grid.Column="3">
                                                        <Label Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.MaSanPham}" Name="lblMaSanPham" />
                                                        <Label Content="(*)" Foreground="Red" Margin="-5,0,0,0" />
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal" Grid.Row="4">
                                                        <Label Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.HinhThucTraLai}" />
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal" Grid.Row="6">
                                                        <Label Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.LoaiTien}" Name="lblLoaiTien" />
                                                        <Label Content="(*)" Foreground="Red" Margin="-5,0,0,0" />
                                                    </StackPanel>
                                                    <telerik:RadComboBox Name="cmbNhomSP" Grid.Column="1" TabIndex="0" IsEditable="True" />
                                                    <TextBox Name="txtMaSanPham" Grid.Column="4" Grid.Row="0" Padding="0,3,0,0" TabIndex="2" IsEnabled="False" />
                                                    <telerik:RadComboBox Name="cmbHinhThuc" Grid.Column="1" Grid.Row="4" TabIndex="4" IsEditable="True"/>
                                                    <telerik:RadComboBox Name="cmbLoaiTien" Grid.Column="1" Grid.Row="6" TabIndex="7" IsEditable="True"/>
                                                    <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.Row="2" >
