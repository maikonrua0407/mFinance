                                                        <Label Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.TanSuatTraLai}" />
                                                    </StackPanel>
                                                    <TextBox Name="txtTenSanPham" Grid.Column="1" Grid.Row="2" Padding="0,3,0,0" TabIndex="3" Grid.ColumnSpan="4"/>
                                                    <Grid Grid.Row="4" Grid.Column="4">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="40"/>
                                                        </Grid.ColumnDefinitions>
                                                        <telerik:RadMaskedNumericInput Mask="#2" Name="numTanSuatTraLai" Placeholder=" " HorizontalAlignment="Stretch" TextMode="PlainText" TabIndex="5" />
                                                        <telerik:RadComboBox Grid.Column="1" Name="cmbTanSuatTraLai" TabIndex="6" IsEditable="True" Margin="5,0,0,0"/>
                                                        <Label Grid.Column="2" Content="{DynamicResource U.HuyDongVon.ucDangKySanPhamCT.1Lan}" HorizontalAlignment="Right" />
                                                    </Grid>

                                                    <StackPanel Grid.Row="6" Grid.Column="3" Orientation="Horizontal" >
                                                        <Label Name="lblKyHan" Content="{StaticResource U.HuyDongVon.ucDangKySanPhamCT.KyHan}"/>
                                                        <Label Content="(*)" Foreground="Red" Margin="-5,0,0,0" Name="lblStarKyHan" Visibility="{Binding Path=IsEnabled, ElementName=numKyHan}"/>
                                                    </StackPanel>


                                                    <Grid Grid.Row="6" Grid.Column="4">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="*"/>
                                                            <ColumnDefinition Width="40"/>
                                                        </Grid.ColumnDefinitions>

                                                        <telerik:RadMaskedNumericInput Mask="#2" Name="numKyHan" Placeholder=" " HorizontalAlignment="Stretch" TextMode="PlainText" TabIndex="5" />
                                                        <telerik:RadComboBox Grid.Column="1" Grid.ColumnSpan="2" Name="cmbKyHan" TabIndex="6" IsEditable="True" Margin="5,0,0,0"/>

                                                    </Grid>
