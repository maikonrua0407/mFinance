                                                    <telerik:GridViewSelectColumn />
                                                    <uc:MyColumn Header="{DynamicResource U.DungChung.STT}" Width="40" MinWidth="40" />
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.MaCTV}" Width="100" MinWidth="150" DataMemberBinding="{Binding MA_NS_HO_SO}"/>
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.TenCTV}" Width="*" MinWidth="200" DataMemberBinding="{Binding TEN_HO_SO}"/>
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.ChucVuCTV}" Width="150" MinWidth="150" DataMemberBinding="{Binding CHUC_VU}"/>
                                                </telerik:RadGridView.Columns>
                                            </telerik:RadGridView>
                                        </DockPanel>
                                    </GroupBox>
                                </DockPanel>
                            </Grid>
                        </ScrollViewer>

                        </telerik:RadTabItem>
                        <telerik:RadTabItem Name="tbiThongTinKiemSoat" Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.ThongTinKiemSoat}" Style="{DynamicResource HeaderTemplate}" Height="{Binding ElementName=grMain, Path=ActualWidth,Converter={uc:MathConverter}, ConverterParameter=x/5}"
