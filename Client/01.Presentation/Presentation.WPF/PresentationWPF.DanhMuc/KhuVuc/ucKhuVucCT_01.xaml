                                                </StackPanel>
                                            </Grid>
                                            <telerik:RadGridView Name="raddgrCanBoQLy" MinHeight="150" MaxHeight="150" ColumnWidth="*" DockPanel.Dock="Top" TabIndex="6" Margin="0,5,0,5"
                AutoGenerateColumns="False" CanUserFreezeColumns="False" RowIndicatorVisibility="Collapsed" ShowGroupPanel="False" IsReadOnly="True" SelectionMode="Extended">
                                                <telerik:RadGridView.Columns>
                                                    <telerik:GridViewSelectColumn />
                                                    <uc:MyColumn Header="{DynamicResource U.DungChung.STT}" Width="40" MinWidth="40" />
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.MaCanBo}" Width="100" MinWidth="100" DataMemberBinding="{Binding MA_NS_HO_SO}"/>
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.TenCanBo}" Width="*" MinWidth="200" DataMemberBinding="{Binding TEN_HO_SO}"/>
                                                    <telerik:GridViewDataColumn Header="{DynamicResource U.DanhMuc.KhuVuc.ucKhuVucCT_01.ChucVu}" Width="150" MinWidth="150" DataMemberBinding="{Binding CHUC_VU}"/>
                                                </telerik:RadGridView.Columns>
                                            </telerik:RadGridView>
