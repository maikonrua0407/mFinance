                            </telerik:RadGridView.Columns>
                        </telerik:RadGridView>
                    </DockPanel>

                </GroupBox>
                <GroupBox Header="{DynamicResource U.TinDungTD.ucPopupThuGocLaiCT.GroupXuLyNopThua}" Margin="3" Padding="10,5,0,5" DockPanel.Dock="Top" Name="grbThongTinNopThua">
