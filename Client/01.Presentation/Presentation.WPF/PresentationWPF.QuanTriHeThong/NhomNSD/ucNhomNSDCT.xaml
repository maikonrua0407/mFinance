        <my:String x:Key="U.QuanTriHeThong.ucNhomNSDCT.TenNhom">Tên nhóm</my:String>
        <my:String x:Key="U.QuanTriHeThong.ucNhomNSDCT.MoTa">Mô tả:</my:String>
