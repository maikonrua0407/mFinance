    </UserControl.Resources>
    <Grid>

        <DockPanel>
            <DockPanel HorizontalAlignment="Stretch" DockPanel.Dock="Top" Margin="3,3,0,3" LastChildFill="False" Name="docToolbar">
                <WrapPanel Name="Toolbar">
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbAdd" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Them}"  SmallImageSource="/Utilities.Common;component/Images/Action/add.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbModify" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Sua}"   SmallImageSource="/Utilities.Common;component/Images/Action/modify.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbDelete" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Xoa}"   SmallImageSource="/Utilities.Common;component/Images/Action/delete.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <Label Name="lbl1" Margin="3,0,0,0" DockPanel.Dock="Left"  BorderThickness="3" Width="2" BorderBrush="#FF170404" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbApprove" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Duyet}"   SmallImageSource="/Utilities.Common;component/Images/Action/approve.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbRefuse" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Tuchoi}"   SmallImageSource="/Utilities.Common;component/Images/Action/refuse.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbCancel" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Thoaiduyet}"   SmallImageSource="/Utilities.Common;component/Images/Action/cancel.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" IsEnabled="False"/>
                    <Label Name="lbl2" Margin="3,0,0,0" DockPanel.Dock="Left"  BorderThickness="3" Width="2" BorderBrush="#FF170404" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbView" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Xem}"   SmallImageSource="/Utilities.Common;component/Images/Action/view.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbSearch" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.TimKiem}"   SmallImageSource="/Utilities.Common;component/Images/Action/search.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbReload" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.LayLai}"  SmallImageSource="/Utilities.Common;component/Images/Action/reload.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbExport" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.XuatExcel}"   SmallImageSource="/Utilities.Common;component/Images/Action/export.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <Label Name="lbl3" Margin="3,0,0,0" DockPanel.Dock="Left"  BorderThickness="3" Width="2" BorderBrush="#FF170404" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbHelp" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.TroGiup}"  SmallImageSource="/Utilities.Common;component/Images/Action/help.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                    <ribbon:RibbonButton Margin="3,0,0,0" Name="tlbClose" DockPanel.Dock="Left" Label="{DynamicResource U.DungChung.Button.Dong}"   SmallImageSource="/Utilities.Common;component/Images/Action/close.png" Style="{StaticResource RibbonButton}" Click="tlbHotKey_Click" />
                </WrapPanel>
            </DockPanel>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="8*"/>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <DockPanel Margin="6,0,0,0">
                        <Border Grid.Row="1" BorderBrush="LightGray" BorderThickness="1">
                            <telerik:RadTreeView IsLineEnabled="True" IsExpandOnSingleClickEnabled="True"
				        IsOptionElementsEnabled="True"
				        IsRootLinesEnabled="True"  x:Name="tvwTree" Padding="5"
				        IsTriStateMode="True">
                                <telerik:RadTreeViewItem Name="item"/>
                            </telerik:RadTreeView>
                        </Border>
                    </DockPanel>
                </Grid>
                <GridSplitter Grid.Column="1" Width="5" ResizeBehavior="PreviousAndNext"/>
                <Grid Grid.Column="2">
                    <Grid>
                        <DockPanel>
                            <Expander DockPanel.Dock="Top" Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.TimKiemNangCao}" Name="expTimKiemNangCao">
                                <GroupBox Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.ThongTinTimKiem}">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="20" />
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="2*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="6" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="6" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="6" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="6" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Label Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.TrangThai}" Grid.Row="0" />
                                        <uc:CheckboxListTrangThaiNghiepVu Name="ucTrangThaiNVu" Grid.Column="1" Grid.ColumnSpan="4" Margin="0,-2,0,0" TabIndex="0" />

                                        <Label Grid.Row="2" Grid.Column="0" Name="lblNhanVien" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.MaNhanVien}"/>
                                        <TextBox Grid.Row="2" Grid.Column="1" Name="txtNhanVien"/>
                                        <Label Grid.Row="2" Grid.Column="3" Name="lblTenNhanVien" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.TenNhanVien}"/>
                                        <TextBox Grid.Row="2" Grid.Column="4" Name="txtTenNhanVien"/>

                                        <Label Grid.Row="4" Grid.Column="0" Name="lblPhongBanCu" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.PhongBanCu}"/>
                                        <telerik:RadComboBox Grid.Row="4" Grid.Column="1" Name="cmbPhongBanCu" IsEditable="True" />
                                        <StackPanel Grid.Row="4" Grid.Column="3" Orientation="Horizontal" >
                                            <Label Name="lblPhongBanMoi" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.PhongBanMoi}"/>                                            
                                        </StackPanel>
                                        <telerik:RadComboBox Grid.Row="4" Grid.Column="4" Name="cmbPhongBanMoi" IsEditable="True" />

                                        <Label Grid.Row="6" Grid.Column="0" Name="lblChucVuCu" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.ChucVuCu}"/>
                                        <telerik:RadComboBox Grid.Row="6" Grid.Column="1" Name="cmbChucVuCu" IsEditable="True" />
                                        <StackPanel Grid.Row="6" Grid.Column="3" Orientation="Horizontal" >
                                            <Label Name="lblChucVuMoi" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.ChucVuMoi}"/>                                            
                                        </StackPanel>
                                        <telerik:RadComboBox Grid.Row="6" Grid.Column="4" Name="cmbChucVuMoi" IsEditable="True" />

                                        <Label Grid.Row="8" Grid.Column="0" Name="lblNgayChuyen" Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.NgayChuyen}"/>
                                        <Grid Grid.Row="8" Grid.Column="1">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="35" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <telerik:RadMaskedDateTimeInput Name="raddtNgayChuyenCongTacTu" Margin="0,0,35,0" HorizontalAlignment="Stretch" Mask="dd/MM/yyyy" TabIndex="4" Text="" Value="{Binding Path=SelectedDate, ElementName=dtpNgayChuyenCongTacTu}"/>
                                            <DatePicker Name="dtpNgayChuyenCongTacTu" Width="30" HorizontalAlignment="Right" TabIndex="5" />
                                            <Label Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.Den}" Grid.Column="1" HorizontalAlignment="Center" />
                                            <telerik:RadMaskedDateTimeInput Name="raddtNgayChuyenCongTacDen" Margin="0,0,35,0" Grid.Column="2" HorizontalAlignment="Stretch" Mask="dd/MM/yyyy" TabIndex="6" Text="" Value="{Binding Path=SelectedDate, ElementName=dtpNgayChuyenCongTacDen}"/>
                                            <DatePicker Name="dtpNgayChuyenCongTacDen" Width="30" HorizontalAlignment="Right" Grid.Column="2" />
                                        </Grid>

                                    </Grid>
                                </GroupBox>
                            </Expander>
                            <Grid DockPanel.Dock="Top">
                                <DockPanel Margin="3">
                                    <telerik:RadNumericUpDown DockPanel.Dock="Right" Name="nudPageSize" Width="60" IsInteger="True" Value="10" TabIndex="7" ValueChanged="nudPageSize_ValueChanged" />
                                    <TextBox Text="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.TimKiem}" Name="txtTimKiemNhanh" TabIndex="6" TextChanged="txtTimKiemNhanh_TextChanged" LostFocus="txtTimKiemNhanh_LostFocus" GotFocus="txtTimKiemNhanh_GotFocus"></TextBox>
                                </DockPanel>
                            </Grid>
                            <Grid DockPanel.Dock="Bottom" Margin="3,0">
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FF889393" Offset="0" />
                                        <GradientStop Color="#FFDEDEDE" Offset="1" />
                                    </LinearGradientBrush>
                                </Grid.Background>
                                <DockPanel>
                                    <StackPanel DockPanel.Dock="Left" Orientation="Horizontal">
                                        <Label Content="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.TongSo}"/>
                                        <Label Margin="-5,0,0,0" Name="lblTongSo"/>
                                    </StackPanel>
                                    <telerik:RadDataPager DockPanel.Dock="Right" PageSize="10" DisplayMode="All" IsTotalItemCountFixed="False" Name="radPage" Source="{Binding Items, ElementName=grChuyenCongTacDS}"/>
                                    <Label/>
                                </DockPanel>
                            </Grid>
                            <telerik:RadGridView Name="grChuyenCongTacDS" ItemsSource="{Binding}" SelectionMode="Extended" AutoGenerateColumns="False" ShowGroupPanel="False" RowIndicatorVisibility="Collapsed" IsReadOnly="True" Margin="3,0" TabIndex="8" MouseDoubleClick="grChuyenCongTacDS_MouseDoubleClick">
                                <telerik:RadGridView.ContextMenu>
                                    <ContextMenu Name="mnuGrid">
                                    </ContextMenu>
                                </telerik:RadGridView.ContextMenu>
                                <telerik:RadGridView.Columns>
                                    <telerik:GridViewDataColumn Name="ID" IsVisible="False" DataMemberBinding="{Binding ID}"/>
                                    <telerik:GridViewSelectColumn MaxWidth="20" MinWidth="20"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridSTT}" Width="50" DataMemberBinding="{Binding STT}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridMaChuyen}" Width="*" DataMemberBinding="{Binding MA_TCHUYEN_CTAC}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridNgayChuyen}" Width="*" DataMemberBinding="{Binding NGAY_HLUC}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridMaNhanVien}" Width="*" DataMemberBinding="{Binding MA_HSO}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridTenNhanVien}" Width="*" DataMemberBinding="{Binding TEN_HSO}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridChucVuCu}" Width="*" DataMemberBinding="{Binding CHUC_VU_CU}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridChucVuMoi}" Width="*" DataMemberBinding="{Binding CHUC_VU_MOI}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridPhongBanCu}" Width="*" DataMemberBinding="{Binding BO_PHAN_CU}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridPhongBanMoi}" Width="*" DataMemberBinding="{Binding BO_PHAN_CU}"/>
                                    <telerik:GridViewDataColumn Header="{DynamicResource U.NhanSu.ChuyenCongTac.ucChuyenCongTacDS.GridTrangThai}" Width="*" DataMemberBinding="{Binding TTHAI_NVU}"/>
                                </telerik:RadGridView.Columns>
                            </telerik:RadGridView>
                        </DockPanel>
                    </Grid>
                </Grid>

            </Grid>

        </DockPanel>
    </Grid>
</UserControl>
