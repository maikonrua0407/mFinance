@model PresentationAspNet.MVC.Error
<div class="page-content">
	<div class="row-fluid">
		<div class="span12">
			<!--PAGE CONTENT BEGINS-->
			<div class="error-container">
				<div class="well">
					<h2 class="green">
						<span class="bigger-125">
                            <i class="icon-random"></i>
                            @Model.TitleError
						</span>
					</h2>

					<hr>
					<h3 class="red smaller">
						<i class="icon-info-sign icon-animated-wrench bigger-125"></i>
						@Model.MessageError
					</h3>

					<div class="space"></div>

					<div>
						<h3 class="lighter smaller">Hướng dẫn:</h3>

						<ul class="unstyled spaced inline bigger-110">
							<li>
								<i class="icon-plus red"></i>
                                Gán quyền làm việc cho tài khoản theo đường dẫn
                                <span class="blue bigger-130">@Model.MenuParent
                                    <i class="icon-hand-right blue"></i>
                                    @Model.MenuChild
                                </span>
							</li>
						</ul>
					</div>

					<hr>
					<div class="space"></div>

					<div class="row-fluid">
						<div class="center">
							<a href="/Home" class="btn btn-primary">
								<i class="icon-arrow-left"></i>
								Trang chủ
							</a>
						</div>
					</div>
				</div>
			</div>

			<!--PAGE CONTENT ENDS-->
		</div><!--/.span-->
	</div><!--/.row-fluid-->
</div>