@using PresentationAspNet.MVC
<div class="row-fluid">
    <div class="span12">
        <div class="widget-box" style="width:490px">
			<table class="table table-striped table-bordered" style="width:100%; border-bottom: none !important">
				<thead>
					<tr>
						<th style="width:100px" class="cssC">Phím tắt</th>
		                <th class="cssL">Chức năng áp dụng</th>		                        
					</tr>
				</thead>						
			</table>
	        <div class="dialogs">
	            <table class="table table-striped table-bordered" style="width:100%">
	                <tbody>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F1</b></td>
                            <td>Xem Trợ giúp chức năng</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F2</b></td>
                            <td>Chọn bản ghi trên popup tìm kiếm</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F3</b></td>
                            <td>Chuyển nhanh tới nhập thông tin khách hàng</td>
                        </tr> 
                        <tr>
                            <td style="width:100px" class="cssC"><b>F4</b></td>
                            <td>Chuyển nhanh tới nhập thông tin nhà cung cấp</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F6</b></td>
                            <td>Kiểm tra tồn kho của mặt hàng
                                <img src="~/Content/assets/imgHotKey/xemton.png" alt="xemton" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F7</b></td>
                            <td>Chuyển nhanh tới nhập thông tin hàng hóa</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F8</b></td>
                            <td>In phiếu
                                <img src="~/Content/assets/imgHotKey/in.png" alt="in" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F9</b></td>
                            <td>Quay lại danh sách phiếu
                                <img src="~/Content/assets/imgHotKey/danhsach.png" alt="danhsach" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>F10</b></td>
                            <td>Thêm mới phiếu
                                <img src="~/Content/assets/imgHotKey/themmoi.png" alt="themmoi" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>CTRL + S</b></td>
                            <td>Lưu phiếu vào hệ thống 
                                <img src="~/Content/assets/imgHotKey/luu.png" alt="luu" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>CTRL + I</b></td>
                            <td>Lưu và in phiếu vào hệ thống 
                                <img src="~/Content/assets/imgHotKey/luu_in.png" alt="luu va in" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>CTRL + F</b></td>
                            <td>Tìm kiếm 
                                <img src="~/Content/assets/imgHotKey/timkiem.png" alt="tim kiem" />
                            </td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b><i class="icon-arrow-up"></i></b></td>
                            <td>Di chuyển đến bản ghi trước trên popup tìm kiếm</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b><i class="icon-arrow-down"></i></b></td>
                            <td>Di chuyển đến bản ghi tiếp theo trên popup tìm kiếm</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b><i class="icon-arrow-left"></i></b></td>
                            <td>Chuyển trang sau trên popup tìm kiếm</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b><i class="icon-arrow-right"></i></b></td>
                            <td>Chuyển trang tiếp trên popup tìm kiếm</td>
                        </tr>
                        <tr>
                            <td style="width:100px" class="cssC"><b>ESC</b></td>
                            <td>Đóng giao diện popup</td>
                        </tr>
                    </tbody>
	            </table> 
	        </div>
		</div>
    </div>
</div>
<script type="text/javascript">
    $(function () {
        $('.dialogs').slimScroll({
            height: '350px'
        });
    });
</script>

